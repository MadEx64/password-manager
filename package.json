{
  "name": "password-manager",
  "version": "1.0.0",
  "description": "Simple CLI password manager with password generator",
  "keywords": [
    "password",
    "manager",
    "generator",
    "cli",
    "command-line",
    "password-manager",
    "password-generator"
  ],
  "main": "index.js",
  "bin": {
    "password-manager": "index.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "start": "node index.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "type": "module",
  "author": "Mickael Beites",
  "license": "ISC",
  "dependencies": {
    "aes-js": "^3.1.2",
    "chalk": "^5.0.1",
    "clipboardy": "^3.0.0",
    "inquirer": "^9.1.2",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "jest": "^29.2.1"
  }
}
